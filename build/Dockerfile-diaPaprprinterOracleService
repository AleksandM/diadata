FROM golang:1.14 as build

WORKDIR $GOPATH

COPY . .

WORKDIR $GOPATH/src/github.com/diadata-org/diadata/cmd/blockchain/ethereum/diaPaprprinterOracleService

RUN go install

FROM gcr.io/distroless/base

COPY --from=build /go/bin/diaPaprprinterOracleService /bin/diaPaprprinterOracleService

ENTRYPOINT ["diaPaprprinterOracleService"]
